inp w
mul x 0
add x z
mod x 26

-- z * (1 + (if (x - 9) == w then 1 else 0) * 25) = -(x * (10 + w))
div z 26

-- z * (1 + (if (x - 9) == w then 1 else 0) * 25) = -(x * (10 + w))
add x -9

-- z * (1 + (if x == w then 1 else 0) * 25) = -(x * (10 + w))
eql x w

-- z * (1 + (if x == w then 0 else 1) * 25) = -(x * (10 + w))
eql x 0

mul y 0

add y 25

mul y x
add y 1

-- z * (1 + x * 25) = -(x * (10 + w))
mul z y

-- z = -(x * (10 + w))
mul y 0

-- z = -(x * (y + 10 + w))
add y w

-- z = -(x * (y + 10)
add y 10

-- z = -(x * y)
mul y x

-- z = -y
add z y
