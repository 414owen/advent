-- z = acc
-- w = input
inp w

-- x = 0
mul x 0

-- x = z
add x z

-- x = z % 26
mod x 26

-- z1 = z / 26
div z 26

-- x = z % 26 - 6
add x -6

eql x w
eql x 0
mul y 0
add y 25
mul y x
add y 1
mul z y
mul y 0
add y w
add y 10
mul y x
add z y


